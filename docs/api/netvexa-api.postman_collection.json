{
  "info": {
    "name": "NETVEXA API",
    "description": "API collection for NETVEXA - AI-powered customer support platform",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "apikey",
    "apikey": [
      {
        "key": "key",
        "value": "X-API-Key",
        "type": "string"
      },
      {
        "key": "value",
        "value": "{{api_key}}",
        "type": "string"
      },
      {
        "key": "in",
        "value": "header",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "base_url",
      "value": "https://api.netvexa.com/api",
      "type": "string"
    },
    {
      "key": "api_key",
      "value": "your_api_key_here",
      "type": "string"
    },
    {
      "key": "agent_id",
      "value": "",
      "type": "string"
    },
    {
      "key": "conversation_id",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register",
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"SecurePass123!\",\n  \"name\": \"John Doe\",\n  \"company_name\": \"Acme Corp\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/register",
              "host": ["{{base_url}}"],
              "path": ["auth", "register"]
            }
          }
        },
        {
          "name": "Login",
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"SecurePass123!\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/login",
              "host": ["{{base_url}}"],
              "path": ["auth", "login"]
            }
          }
        },
        {
          "name": "Get Profile",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/auth/profile",
              "host": ["{{base_url}}"],
              "path": ["auth", "profile"]
            }
          }
        },
        {
          "name": "Create API Key",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Production API Key\",\n  \"scopes\": [\"read:agents\", \"write:agents\", \"read:conversations\", \"write:knowledge\"]\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/api-keys",
              "host": ["{{base_url}}"],
              "path": ["auth", "api-keys"]
            }
          }
        }
      ]
    },
    {
      "name": "Agents",
      "item": [
        {
          "name": "List Agents",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/agents?page=1&limit=10",
              "host": ["{{base_url}}"],
              "path": ["agents"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            }
          }
        },
        {
          "name": "Create Agent",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('agent_id', response.id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Customer Support Bot\",\n  \"description\": \"Helps customers with product inquiries\",\n  \"prompt\": \"You are a helpful customer support agent. Be friendly, professional, and concise in your responses.\",\n  \"model\": \"gpt-3.5-turbo\",\n  \"temperature\": 0.7,\n  \"welcome_message\": \"Hello! How can I help you today?\",\n  \"suggested_questions\": [\n    \"What are your business hours?\",\n    \"How do I track my order?\",\n    \"What is your return policy?\"\n  ],\n  \"collect_email\": true,\n  \"brand_color\": \"#2563eb\",\n  \"position\": \"bottom-right\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/agents",
              "host": ["{{base_url}}"],
              "path": ["agents"]
            }
          }
        },
        {
          "name": "Get Agent",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/agents/{{agent_id}}",
              "host": ["{{base_url}}"],
              "path": ["agents", "{{agent_id}}"]
            }
          }
        },
        {
          "name": "Update Agent",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Updated Support Bot\",\n  \"temperature\": 0.8,\n  \"welcome_message\": \"Welcome! I'm here to help. What can I do for you?\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/agents/{{agent_id}}",
              "host": ["{{base_url}}"],
              "path": ["agents", "{{agent_id}}"]
            }
          }
        },
        {
          "name": "Test Agent",
          "request": {
            "method": "POST",
            "url": {
              "raw": "{{base_url}}/agents/{{agent_id}}/test-message?message=What are your business hours?",
              "host": ["{{base_url}}"],
              "path": ["agents", "{{agent_id}}", "test-message"],
              "query": [
                {
                  "key": "message",
                  "value": "What are your business hours?"
                }
              ]
            }
          }
        },
        {
          "name": "Deploy Agent",
          "request": {
            "method": "POST",
            "url": {
              "raw": "{{base_url}}/agents/{{agent_id}}/deploy",
              "host": ["{{base_url}}"],
              "path": ["agents", "{{agent_id}}", "deploy"]
            }
          }
        },
        {
          "name": "Delete Agent",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "{{base_url}}/agents/{{agent_id}}",
              "host": ["{{base_url}}"],
              "path": ["agents", "{{agent_id}}"]
            }
          }
        }
      ]
    },
    {
      "name": "Knowledge",
      "item": [
        {
          "name": "Upload Document",
          "request": {
            "method": "POST",
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": "/path/to/document.pdf"
                },
                {
                  "key": "agent_id",
                  "value": "{{agent_id}}",
                  "type": "text"
                },
                {
                  "key": "title",
                  "value": "Product Documentation",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/knowledge/ingest/file",
              "host": ["{{base_url}}"],
              "path": ["knowledge", "ingest", "file"]
            }
          }
        },
        {
          "name": "Import from URL",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"url\": \"https://example.com/docs\",\n  \"agent_id\": \"{{agent_id}}\",\n  \"title\": \"Online Documentation\",\n  \"max_depth\": 2\n}"
            },
            "url": {
              "raw": "{{base_url}}/knowledge/ingest/url",
              "host": ["{{base_url}}"],
              "path": ["knowledge", "ingest", "url"]
            }
          }
        },
        {
          "name": "List Documents",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/knowledge/documents?agent_id={{agent_id}}&page=1",
              "host": ["{{base_url}}"],
              "path": ["knowledge", "documents"],
              "query": [
                {
                  "key": "agent_id",
                  "value": "{{agent_id}}"
                },
                {
                  "key": "page",
                  "value": "1"
                }
              ]
            }
          }
        },
        {
          "name": "Delete Document",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "{{base_url}}/knowledge/documents/{{document_id}}",
              "host": ["{{base_url}}"],
              "path": ["knowledge", "documents", "{{document_id}}"]
            }
          }
        }
      ]
    },
    {
      "name": "Conversations",
      "item": [
        {
          "name": "List Conversations",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    if (response.conversations && response.conversations.length > 0) {",
                  "        pm.collectionVariables.set('conversation_id', response.conversations[0].id);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/agents/{{agent_id}}/conversations?status=active&limit=20",
              "host": ["{{base_url}}"],
              "path": ["agents", "{{agent_id}}", "conversations"],
              "query": [
                {
                  "key": "status",
                  "value": "active"
                },
                {
                  "key": "limit",
                  "value": "20"
                }
              ]
            }
          }
        },
        {
          "name": "Get Conversation",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/conversations/{{conversation_id}}",
              "host": ["{{base_url}}"],
              "path": ["conversations", "{{conversation_id}}"]
            }
          }
        },
        {
          "name": "Get Messages",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/conversations/{{conversation_id}}/messages",
              "host": ["{{base_url}}"],
              "path": ["conversations", "{{conversation_id}}", "messages"]
            }
          }
        },
        {
          "name": "Resolve Conversation",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"resolution_notes\": \"Customer issue resolved successfully\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/conversations/{{conversation_id}}/resolve",
              "host": ["{{base_url}}"],
              "path": ["conversations", "{{conversation_id}}", "resolve"]
            }
          }
        }
      ]
    },
    {
      "name": "Analytics",
      "item": [
        {
          "name": "Get Agent Analytics",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/agents/{{agent_id}}/analytics?start_date=2024-01-01&end_date=2024-01-31",
              "host": ["{{base_url}}"],
              "path": ["agents", "{{agent_id}}", "analytics"],
              "query": [
                {
                  "key": "start_date",
                  "value": "2024-01-01"
                },
                {
                  "key": "end_date",
                  "value": "2024-01-31"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Billing",
      "item": [
        {
          "name": "Get Subscription",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/billing/subscription",
              "host": ["{{base_url}}"],
              "path": ["billing", "subscription"]
            }
          }
        },
        {
          "name": "Get Usage Stats",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/billing/usage?period=current",
              "host": ["{{base_url}}"],
              "path": ["billing", "usage"],
              "query": [
                {
                  "key": "period",
                  "value": "current"
                }
              ]
            }
          }
        },
        {
          "name": "Upgrade Subscription",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"tier\": \"growth\",\n  \"payment_method_id\": \"pm_card_visa\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/billing/subscription/upgrade",
              "host": ["{{base_url}}"],
              "path": ["billing", "subscription", "upgrade"]
            }
          }
        }
      ]
    },
    {
      "name": "Webhooks",
      "item": [
        {
          "name": "List Webhooks",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/webhooks",
              "host": ["{{base_url}}"],
              "path": ["webhooks"]
            }
          }
        },
        {
          "name": "Create Webhook",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"url\": \"https://your-app.com/netvexa-webhook\",\n  \"events\": [\"conversation.started\", \"conversation.message\", \"lead.captured\"],\n  \"secret\": \"webhook_secret_key\",\n  \"active\": true\n}"
            },
            "url": {
              "raw": "{{base_url}}/webhooks",
              "host": ["{{base_url}}"],
              "path": ["webhooks"]
            }
          }
        }
      ]
    }
  ]
}